{
    "name": "ComeFromTwitter",
    "version": "1.0",
    "description": "Click the button to modify referrer to twitter!",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": {
            "16": "icon16.png",
            "32": "icon32.png",
            "48": "icon48.png",
            "64": "icon64.png",
            "128": "icon128.png"
        },
        "default_title": "Redirect!"
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "activeTab",
        "<all_urls>"
    ],

    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "64": "icon64.png",
        "128": "icon128.png"
    },
    "manifest_version": 2
}
